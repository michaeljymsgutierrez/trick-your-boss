{
  "manifest_version": 3,
  "name": "Keep Online",
  "version": "1.0",
  "description": "Simulat user interaction to keep system busy",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
